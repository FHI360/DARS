    <property name="fundingBody" column="FUNDINGBODY" type="string" />
    <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="string" />
    <property name="organizationUnit" column="ORGANIZATIONUNIT" type="string" />
    <property name="organizationUnitGroup" column="ORGUNITGROUP" type="string" />
    <property name="organizationUnitOwnership" column="ORGANIZATIONUNITOWNERSHIP" type="string" />
