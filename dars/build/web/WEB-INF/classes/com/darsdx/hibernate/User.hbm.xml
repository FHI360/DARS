<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.darsdx.business.User" table="DARSUSER">
    <id name="username" column="USERNAME" type="string" />
    <property name="firstName" column="FIRSTNAME" type="string" />
    <property name="lastName" column="LASTNAME" type="string" />
    <property name="password" column="PWD" type="string" />
    <property name="userroleId" column="USERROLEID" type="string" />
    <property name="dateCreated" column="DATECREATED" type="string" />
    <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="string" />
    </class>
</hibernate-mapping>
