<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.darsdx.business.DatabaseConnection" table="DATABASECONNECTION">
    <id name="id" column="ID" type="string"/>
    <property name="connectionName" column="CONNECTIONNAME" type="string"/>
    <property name="databaseName" column="DATABASENAME" type="string"/>
    <property name="databaseURL" column="DBURL" type="string"/>
    <property name="dateCreated" column="DATECREATED" type="date"/>
    <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="date"/>
    <property name="userName" column="USERNAME" type="string"/>
    <property name="password" column="PASSWORD" type="string"/>
    <property name="portNumber" column="PORTNUMBER" type="int"/>
    <property name="recordedBy" column="RECORDEDBY" type="string"/>
  </class>
</hibernate-mapping>
